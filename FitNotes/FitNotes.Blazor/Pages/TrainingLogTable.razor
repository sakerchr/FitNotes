<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Exercise</th>
            <th>Weigth</th>
            <th>Sets</th>
            <th>Reps</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var trainingLog in TrainingLogBackup.TrainingLogs.OrderByDescending(tl => tl.Date))
        {
            <TrainingLogTableEntry TrainingLog="@trainingLog" />
        }
    </tbody>
</table>


@code {
    [CascadingParameter]
    public TrainingLogBackup? TrainingLogBackup { get; set; }
}